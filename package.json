{
  "name": "antigravity-agent",
  "version": "1.0.0",
  "description": "A production-grade AI agent wrapper for Gemini with MCP integration",
  "type": "module",
  "main": "dist/agent.js",
  "types": "dist/agent.d.ts",
  "scripts": {
    "dev": "tsx src/agent.ts",
    "dev:swarm": "tsx src/swarm-demo.ts",
    "build": "tsc",
    "start": "node dist/agent.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src tests --ext .ts"
